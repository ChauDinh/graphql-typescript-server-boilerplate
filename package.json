{
  "name": "new-typeorm-project",
  "version": "0.0.1",
  "description": "Awesome project developed with TypeORM.",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/ioredis": "^4.16.0",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.4",
    "@types/node-fetch": "^2.5.7",
    "@types/uuid": "^7.0.3",
    "@types/yup": "^0.28.0",
    "jest": "^25.5.4",
    "nodemon": "^2.0.3",
    "start-server-and-test": "^1.11.0",
    "ts-jest": "^25.4.0",
    "ts-node": "6",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "3.8.3"
  },
  "dependencies": {
    "@gql2ts/from-schema": "^2.0.0-4",
    "bcryptjs": "^2.4.3",
    "gql2ts": "^1.10.1",
    "graphql-import": "^1.0.2",
    "graphql-request": "^1.8.2",
    "graphql-tools": "^5.0.0",
    "graphql-yoga": "^1.18.3",
    "ioredis": "^4.16.3",
    "node-fetch": "^2.6.0",
    "pg": "^8.0.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "0.2.24",
    "uuid": "^8.0.0",
    "yup": "^0.28.5"
  },
  "scripts": {
    "start": "NODE_ENV=development nodemon --exec ts-node src/index.ts",
    "test-server": "NODE_ENV=test ts-node src/index.ts",
    "gen-schema-types": "gql2ts src/schema.graphql -o src/types/schema.d.ts",
    "test": "NODE_ENV=test jest --watch",
    "ci": "start-server-and-test test-server http://localhost:4000 test"
  },
  "license": "MIT",
  "jest": {
    "globalSetup": "./src/jestGlobalSetup/callSetup.js",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
